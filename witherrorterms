from decimal import Decimal, getcontext
import multiprocessing as mp
import os
import time

# Customizable parameters
X_VALUES = [1300]  # Add or modify exponents here (n will be 10^x)
E_VALUE = "2."  # More accurate e value
PRECISION = 16000  # Set your desired precision here

# Set precision for Decimal calculations
getcontext().prec = PRECISION

def set_process_priority():
    # Set process to high priority on macOS
    os.nice(10)  # Lower nice value means higher priority

def calculate_term(term_type, x, e_value_decimal=None):
    set_process_priority()
    print(f"Calculating {term_type} for x = {x}")
    n = Decimal(10) ** x
    
    if term_type == "initial_approximation":
        e_approx1 = (Decimal(1) + Decimal(1)/n) ** n
        e_approx2 = Decimal(1) / ((Decimal(1) - Decimal(1)/n) ** n)
        return (e_approx2 - e_approx1) * n
    elif term_type == "error_term_1":
        constant = Decimal(7) / Decimal(8)
        return constant * e_value_decimal / (n * n)
    elif term_type == "error_term_2":
        constant = Decimal(959) / Decimal(1152)
        return constant * e_value_decimal / (n ** 4)
    elif term_type == "error_term_3":
        constant = Decimal(67223) / Decimal(82944)
        return constant * e_value_decimal / (n ** 6)


def approximate_e_advanced(x, e_value_decimal):
    print(f"Starting calculations for x = {x}")
    start_time = time.time()
    
    with mp.Pool(processes=7) as pool:  # Increased to 7 processes for the new term
        results = pool.starmap(calculate_term, [
            ("initial_approximation", x),
            ("error_term_1", x, e_value_decimal),
            ("error_term_2", x, e_value_decimal),
            ("error_term_3", x, e_value_decimal),

        ])
    
    processed_difference, error_term_1, error_term_2, error_term_3 = results
    final_result = processed_difference - error_term_1 - error_term_2 - error_term_3 
    error_from_true_e = final_result - e_value_decimal
    
    end_time = time.time()
    print(f"Calculations for x = {x} completed in {end_time - start_time:.2f} seconds")
    return final_result, error_from_true_e

if __name__ == '__main__':
    print("Starting enhanced e approximation algorithm")
    
    # Optimize for macOS
    mp.set_start_method('fork')

    # Main execution
    e_value_decimal = Decimal(E_VALUE)

    print(f"Processing {len(X_VALUES)} X_VALUES: {X_VALUES}")
    overall_start_time = time.time()

    results = []
    for x in X_VALUES:
        result = approximate_e_advanced(x, e_value_decimal)
        results.append((x, result))

    overall_end_time = time.time()
    print(f"All calculations completed in {overall_end_time - overall_start_time:.2f} seconds")

    for x, (result, error_from_true_e) in results:
        error = abs(result - e_value_decimal)
        correct_digits = abs(error.log10()).quantize(Decimal('1.'))
        relative_error = abs(error / e_value_decimal)
        
        print(f"\nResults for x = {x} (n = 10^{x}):")
        print(f"  Approximation: {result}")
        print(f"  Absolute Error: {error}")
        print(f"  Relative Error: {relative_error}")
        print(f"  Correct Digits: {correct_digits}")
        print(f"  Error from true e: {error_from_true_e}")

    print("\nProgram completed successfully")
