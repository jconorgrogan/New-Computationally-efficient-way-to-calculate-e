from decimal import Decimal, getcontext

# Customizable parameters
X_VALUES = [3000]  # Add or modify exponents here (n will be 10^x)
E_VALUE = "2."  # Add your desired e value here
PRECISION = 13000  # Set your desired precision here

# Set precision for Decimal calculations
getcontext().prec = PRECISION

# Precompute constants
DECIMAL_1 = Decimal(1)
DECIMAL_7_8 = Decimal('0.875')
DECIMAL_959_1152 = Decimal('0.8324652777777777777777777777777777777777777777777778')
DECIMAL_64223_82944 = Decimal('0.7742928864569536423841059602649006622516556291390728')

def approximate_e_advanced(x, e_value_decimal):
    n = Decimal(10) ** x
    
    # Calculate initial approximations
    e_approx1 = (DECIMAL_1 + DECIMAL_1/n) ** n
    e_approx2 = DECIMAL_1 / ((DECIMAL_1 - DECIMAL_1/n) ** n)
    
    # Calculate the difference and process it
    processed_difference = (e_approx2 - e_approx1) * n
    
    # Error terms
    error_term_1 = DECIMAL_7_8 * e_value_decimal / (n * n)
    error_term_2 = DECIMAL_959_1152 * e_value_decimal / (n ** 4)
    error_term_3 = DECIMAL_64223_82944 * e_value_decimal / (n ** 7)
    
    # Final result
    final_result = processed_difference - error_term_1 - error_term_2 - error_term_3
    
    # Calculate error from true value of e
    error_from_true_e = final_result - e_value_decimal
    
    return final_result, error_from_true_e

# Main execution
e_value_decimal = Decimal(E_VALUE)

for x in X_VALUES:
    result, error_from_true_e = approximate_e_advanced(Decimal(x), e_value_decimal)
    error = abs(result - e_value_decimal)
    print(f"x = {x} (n = 10^{x}):")
    print(f"  Approximation: {result}")
    print(f"  Error: {error}")
    print(f"  Correct digits: {abs(error.log10()).quantize(Decimal('1.'))}")
    print(f"  Error from true e: {error_from_true_e}")
    print()
